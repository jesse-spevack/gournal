# Parse parameters
box_num = box.to_i
fill_type, fill_num = fill.split('_') if fill

# Get the box path
box_path = box_paths[box_num] || box_paths[0]

# Label text
label_text = "Box-#{box_num}"
if fill
  label_text += " + #{fill.capitalize.gsub('_', '-')}"
end
%>

<div class="checkbox-item">
  <div class="checkbox-custom">
    <span class="checkbox-box">
      <svg viewBox="0 0 24 24">
        <path class="box-path" d="<%= box_path %>" />
        
        <% if fill_type == "blotch" && blotch_patterns[fill_num.to_i] %>
          <% blotch = blotch_patterns[fill_num.to_i] %>
          <!-- <%= blotch[:comment] %> -->
          <g style="fill: var(--ink-primary); opacity: <%= blotch[:opacity] %>;">
            <path d="<%= blotch[:path] %>" />
          </g>
        <% end %>
      </svg>
    </span>
    
    <% if fill_type == "x" && x_patterns[fill_num.to_i] %>
      <% x_mark = x_patterns[fill_num.to_i] %>
      <span class="x-marks-container">
        <span class="x-mark show">
          <svg viewBox="0 0 24 24">
            <path class="x-path-1" style="stroke-width: <%= x_mark[:path_1][:stroke_width] %>; opacity: <%= x_mark[:path_1][:opacity] %>;" d="<%= x_mark[:path_1][:path] %>" />
            <path class="x-path-2" style="stroke-width: <%= x_mark[:path_2][:stroke_width] %>; opacity: <%= x_mark[:path_2][:opacity] %>;" d="<%= x_mark[:path_2][:path] %>" />
          </svg>
        </span>
      </span>
    <% end %>
  </div>
  <span class="checkbox-label"><%= label_text %></span>
</div>